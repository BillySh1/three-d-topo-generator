{
  "name": "@idg/topo-panel",
  "version": "1.2.1",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "tslint 'packages/**/*.ts' 'packages/**/*.ts'",
    "ts": "tsc && cpx packages/**/*.less dist/es/packages && cpx packages/**/*.png dist/es/packages && cpx packages/**/*.svg dist/es/packages && cpx packages/**/*.js dist/es/packages && cpx packages/**/*.vue dist/es/packages",
    "lib": "vue-cli-service build --target lib --name IdgSeed --filename main --entry packages/index.ts --report",
    "pack": "npm run lib && npm run ts",
    "test": "mocha",
    "mock": "cpx  **/__mocks__/*.js  dist",
    "generate": "idg-helper",
    "commit": "git add . && git-cz",
    "changelog": "node ./scripts/changelog.js",
    "precommit": "npm run lint"
  },
  "module": "dist/es/packages/index.js",
  "dependencies": {
  },
  "main": "dist/main.common.js",
  "typings": "dist/es/packages/index.d.ts",
  "files": [
    "packages",
    "dist",
    "types"
  ],
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@idg/cli-config": "^1.1.3",
    "@idg/design": "^1.2.1",
    "@idg/fastclick": "^0.1.1",
    "@idg/helper": "^3.2.4",
    "@idg/idg": "^2.10.23",
    "@idg/iview": "^4.4.11",
    "@idg/mocker": "^1.0.1",
    "@idg/vc-trigger": "^1.0.17",
    "@sentry/browser": "^6.2.0",
    "@sentry/tracing": "^6.2.0",
    "@sentry/vue": "^6.2.3",
    "@types/lodash": "^4.14.135",
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-pwa": "^3.8.0",
    "@vue/cli-plugin-typescript": "^3.8.0",
    "@vue/cli-service": "^4.3.1",
    "axios": "^0.19.0",
    "babel-core": "7.0.0-bridge.0",
    "chalk": "^3.0.0",
    "commitizen": "^4.1.2",
    "conventional-changelog": "^3.1.21",
    "cpx": "^1.5.0",
    "cz-customizable": "^6.2.0",
    "husky": "^4.2.5",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "localforage": "^1.7.3",
    "lodash": "^4.17.15",
    "mocha": "^6.1.4",
    "mockjs": "^1.0.1-beta3",
    "nyc": "^14.1.1",
    "register-service-worker": "^1.6.2",
    "svg-loader": "^0.0.2",
    "tailwindcss": "^1.1.4",
    "tempfile": "^3.0.0",
    "typescript": "3.8.3",
    "typescript-tslint-plugin": "^0.5.5",
    "vue": "^2.6.10",
    "vue-class-component": "^7.1.0",
    "vue-i18n": "^8.12.0",
    "vue-property-decorator": "^8.2.1",
    "vue-router": "^3.0.7",
    "vue-svg-loader": "^0.16.0",
    "vue-template-compiler": "^2.6.10",
    "vuex": "^3.1.1"
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "license": "MIT"
}
